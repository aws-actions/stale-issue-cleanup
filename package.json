{
  "name": "stale-issue-cleanup",
  "version": "2.0.0",
  "description": "GitHub Action that manages issue lifecycle based on issue labels",
  "private": true,
  "main": "lib/entrypoint.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "release": "ncc build --source-map --license licenses.txt",
    "test": "npx jest --coverage",
    "all": "npm run build && npm run lint && npm run test && npm run release",
    "clean": "rimraf lib dist",
    "update-tests": "node test/bin/record.js && node test/bin/normalize.mjs --fixtures test/fixtures"
  },
  "keywords": [
    "labels",
    "lifecycle",
    "issues"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aws-actions/stale-issue-cleanup"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aws-actions/stale-issue-cleanup/issues"
  },
  "homepage": "https://github.com/aws-actions/stale-issue-cleanup/#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@jest/globals": "^28.0.3",
    "@octokit/fixtures": "^22.0.2",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.30",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "nock": "^13.2.4",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "yargs": "^17.4.1"
  },
  "dependencies": {
    "@actions/core": "^1.7.0",
    "@actions/github": "^5.0.1"
  }
}
